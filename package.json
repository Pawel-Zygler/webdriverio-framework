{
  "name": "webdriverio-framework",
  "version": "1.0.0",
  "description": "webdriver io automation framework",
  "main": "index.js",
  "scripts": {
    "test": "wdio --spec ./test/specs/automation-test-store/**/*.spec.js,./test/specs/folder2/**/*.spec.js",
    "wdio": "wdio run ./wdio.conf.js",
    "webdriver": "npx wdio --spec test/specs/webdriver-university/**/*.spec.js",
    "webdriver-uat": "npx wdio config/wdio.uat.conf.js--spec test/specs/webdriver-university/contact-us.spec.js",
    "smoke-suite": "npx wdio --suite smoke",
    "b": "npx wdio --spec ./test/specs/automation-test-store/add-items-to-basket.spec.js",
    "rh": "npx wdio --spec ./test/specs/automation-test-store/login/register-user-happy.spec.js",
    "ruh": "npx wdio --spec ./test/specs/automation-test-store/login/register-user-unhappy.spec.js",
    "jest": "jest",
    "cch": "npx wdio --spec ./test/specs/automation-test-store/homePage/currency-change.spec.js",
    "vh": "npx wdio --spec ./test/specs/automation-test-store/homePage/visit-social-media-happy.spec.js",
    "lh": "npx wdio --spec ./test/specs/automation-test-store/login/login-happy.spec.js",
    "luh": "npx wdio --spec ./test/specs/automation-test-store/login-unhappy.spec.js",
    "fh": "npx wdio --spec ./test/specs/automation-test-store/forgot-password-happy.spec.js",
    "fuh": "npx wdio --spec ./test/specs/automation-test-store/forgot-password-unhappy.spec.js",
    "flh": "npx wdio --spec ./test/specs/automation-test-store/forgot-login-happy.spec.js",
    "fluh": "npx wdio --spec ./test/specs/automation-test-store/forgot-login-unhappy.spec.js",
    "smh": "npx wdio --spec ./test/specs/automation-test-store/visit-social-media-happy.spec.js"
  },
  "author": "pawel",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@wdio/allure-reporter": "^8.3.5",
    "@wdio/cli": "^8.3.5",
    "@wdio/local-runner": "^8.3.5",
    "@wdio/mocha-framework": "^8.3.0",
    "@wdio/selenium-standalone-service": "^8.5.1",
    "@wdio/spec-reporter": "^8.3.0",
    "chromedriver": "^114.0.0",
    "geckodriver": "^3.2.0",
    "jest": "^29.5.0",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-geckodriver-service": "^4.1.0"
  },
  "dependencies": {
    "allure-commandline": "^2.21.0"
  }
}
