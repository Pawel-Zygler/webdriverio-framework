    "@wdio/mocha-framework": "^8.11.0",
    "@wdio/selenium-standalone-service": "^8.5.1",
    "@wdio/spec-reporter": "^8.11.2",
    "chromedriver": "^116.0.0",
    "geckodriver": "^3.2.0",
    "jest": "^29.5.0",
    "wdio-chromedriver-service": "^8.1.1",
